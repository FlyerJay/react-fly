{
  "name": "react-fly",
  "version": "1.0.0",
  "description": "react framework",
  "main": "index.js",
  "scripts": {
    "test": "make test",
    "start": "npm run dev:local",
    "remote": "npm run dev:test",
    "dev:local": "cross-env NODE_ENV=development node build/webpack.dev.js --hot",
    "dev:test": "cross-env NODE_ENV=development node build/webpack.dev.js --hot --deploy",
    "release": "npm run deploy:online",
    "deploy:test": "cross-env NODE_ENV=production node build/webpack.release.js --watch --uglify --deploy=test",
    "deploy:online": "cross-env NODE_ENV=production node build/webpack.release.js --uglify --deploy=online"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlyerJay/react-fly.git"
  },
  "keywords": [
    "react"
  ],
  "author": "flyerjay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlyerJay/react-fly/issues"
  },
  "homepage": "https://github.com/FlyerJay/react-fly#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-import": "^1.1.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "ftp": "^0.3.10",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "imagemin-mozjpeg": "^6.0.0",
    "mozjpeg": "^4.1.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "antd-mobile": "^0.9.15",
    "flex.css": "^1.1.6",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
